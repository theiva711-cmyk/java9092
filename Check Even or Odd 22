import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        int N;
        N=s.nextInt();
        if(N%2==0) 
        {
            System.out.println("Even");
        }
        else
        {
            System.out.println("Odd");
        }
            
          
    }
}
